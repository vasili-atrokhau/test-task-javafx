<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.DatePicker?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="CENTER" prefHeight="248.0" prefWidth="473.0" spacing="20.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.atrokhau.vasili.controller.EditFormController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
   <Label text="Name:" />
   <TextField fx:id="nameField" prefHeight="27.0" prefWidth="237.0" />
   <Label text="Age:" />
   <TextField fx:id="ageField" prefHeight="27.0" prefWidth="237.0" />
   <Label text="Birthday:" />
   <DatePicker fx:id="birthdayField" />
   <Button fx:id="saveButton" mnemonicParsing="false" onAction="#saveButtonAction" text="Save" />
   <Button fx:id="cancelButton" layoutX="224.0" layoutY="282.0" mnemonicParsing="false" onAction="#closeButton" text="Cancel" />
</VBox>
